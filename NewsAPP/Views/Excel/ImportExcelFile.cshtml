
@{
    ViewData["Title"] = "ImportExcelFile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="mt-0"> Excel Task </h1>

<div class="container mt-3">
    <div class="row">
        <div class="col-6 offset-3">
            <form method="post" asp-action="ImportExcelFile" asp-controller="Excel" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="m-2 p-2">Upload Excel File...</label>
                    <input class="m-2 p-2 form-control" type="file" name="formfile" accept=".csv, .xls, .xlsx" />
                    <input class="m-2 float-end p-2 btn btn-primary btn-sm" type="submit" />
                </div>
            </form>
        </div>
    </div>
</div>



